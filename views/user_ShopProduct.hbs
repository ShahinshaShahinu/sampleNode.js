                console.log(payload)

                searchresults.innerHTML = '';



                if (payload.length < 1) {
                    searchresults.innerHTML = '<br><br><p>Sorry .Nothing Found</p>'
                    return;



                }

                payload.forEach((item, index) => {

                    if (index > 2) return;
                    if (index > 0) searchresults.innerHTML += '<br><br>';

                    searchresults.innerHTML += `<div style='height:3.5rem;    left: 1rem; position: relative; ' onclick="window.location.href='/user-ShowProduct/${item._id}'"class="d-flex"><img src='/productsimg/${item.ProductImage[0]}' width='50rem'><p>${item.ProductName}</p> </div><div><p>₹${item.ProductPrice}</p></div>`

                })


                return;
            })
            
            searchresults.innerHTML = '';
        }

    </script>



